<%- include('../partials/html-head') %>
<%- include('../partials/nav') %>

<main>
  <% if (profile.avatar) { %>
    <div>
      <img 
        class="profile-avatar"
        src="<%= profile.avatar %>" 
        alt="<%= profile.name %> avatar"
      >
    </div>
  <% } %>
  <h1>It's <%= isSelf ? "you" : profile.name %>!</h1>
  <h2>Games beaten:</h2>
  <% profile.gamesBeaten.forEach(game => { %>
    <div>
        <%= game.game.name %>
    </div>
  <% }) %>
</main>

<%- include('../partials/footer') %>